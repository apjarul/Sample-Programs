#Code1:
s='python'
print(s[::-1])

#Code2:
s='python'
l=len(s)
for i in range(-1,(-l-1),-1):
    print(s[i])
    
#Code3:
s='python'
l=len(s)
for i in range(l-1,-1,-1):
    print(s[i])

#code4:
s='python'
s1=""
length=len(s)-1
while length>=0:
    s1=s1+s[length]
    length-=1
print(s1)

    

    
